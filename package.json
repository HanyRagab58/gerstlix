{
  "name": "gerstlix",
  "description": "Modern ES6+ API client for Gerstlix.com",
  "author": "mccormickk",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "docs": "typedoc --out docs src"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "qs": "^6.14.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/node": "^20.19.9",
    "@types/qs": "^6.14.0",
    "typedoc": "^0.28.9",
    "typescript": "^5.9.2",
    "tsup": "8.3.5"
  }
}
